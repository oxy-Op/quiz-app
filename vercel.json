{
"version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "destination": "/projects/anime-quiz"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/projects/anime-quiz",
      "statusCode": 301
    }
  ]
}